---
import { useEffect, useState } from 'react';

// Define la interfaz
interface GazeEvent {
  docX: number;
  docY: number;
  state: number;
}

// Esta funci칩n se ejecutar치 en el lado del cliente
let gazeevents: GazeEvent[] = [];

if (typeof window !== 'undefined') {
  const searchParams = new URLSearchParams(window.location.search);
  const gazeeventsData = searchParams.get('gazeevents');
  gazeevents = gazeeventsData ? JSON.parse(decodeURIComponent(gazeeventsData)) : [];
}
---

<html>
  <head>
    <title>Revisi칩n de Gazeevents</title>
  </head>
  <body>
    <h1>Revisi칩n de Gazeevents</h1>
    <ul>
      {gazeevents.map((event: GazeEvent, index: number) => (
        <li key={index.toString()}>
          X: {event.docX}, Y: {event.docY}, Estado: {event.state}
        </li>
      ))}
    </ul>
  </body>
</html>
