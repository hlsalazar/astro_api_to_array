---
const gazeevents = Astro.url.searchParams.get('gazeevents') || '';
const decodedData = gazeevents ? decodeURIComponent(gazeevents) : '';
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Astro App</title>
</head>
<body>
    <h1>Gaze Events Data:</h1>
    {decodedData ? <pre>{decodedData}</pre> : <p>No data received</p>}
</body>
</html>
